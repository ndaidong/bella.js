{
  "version": "9.2.0",
  "name": "bellajs",
  "description": "A useful helper for any javascript program",
  "homepage": "https://www.npmjs.com/package/bellajs",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/bellajs"
  },
  "author": "@ndaidong",
  "main": "./dist/bella.js",
  "module": "./src/main",
  "browser": "./dist/bella.min.js",
  "engines": {
    "node": ">= 10.14.2"
  },
  "scripts": {
    "lint": "eslint ./src",
    "pretest": "npm run lint && npm run build",
    "test": "tap tests/start.js --coverage --reporter=spec --coverage-report=html --no-browser",
    "citest": "tap tests/start.js --reporter=text-lcov | coveralls",
    "coveralls": "./node_modules/.bin/coveralls",
    "build": "gccmin -e src/main.js -n bella -o dist -p package.json",
    "reset": "node reset"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint-config-goes": "^1.1.8",
    "gcc-min": "^7.2.0",
    "is": "^3.3.0",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "sinon": "^9.0.2",
    "tap": "^14.10.7"
  },
  "keywords": [
    "detection",
    "manipulation",
    "templating",
    "utilities"
  ],
  "license": "MIT"
}
